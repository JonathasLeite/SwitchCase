using System;

namespace VS_WS
{
    class Program
    {
        static void Main(string[] args)
        {
            int x = int.Parse(Console.ReadLine());
            string Dia;

            switch (x)
            {
                case 1:
                    Dia = "Domingo";
                    break;

                case 2:
                    Dia = "Segunda";
                    break;

                case 3:
                    Dia = "terça";
                    break;

                case 4:
                    Dia = "Quarta";
                    break;

                case 5:
                    Dia = "Quinta";
                    break;

                case 6:
                    Dia = "Sexta";
                    break;

                default:
                    Dia = "Sábado";
                    break;
       
            }

            Console.WriteLine(Dia);
        }
    }
}
